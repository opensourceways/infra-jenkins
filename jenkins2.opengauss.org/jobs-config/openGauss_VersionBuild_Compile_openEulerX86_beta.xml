    </scms>
  </scm>
  <assignedNode>x86-openEuler-build</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <customWorkspace>/usr1/gauss_jenkins/jenkins/workspace/</customWorkspace>
  <builders>
    <hudson.tasks.Shell>
      <command>
cd $WORKSPACE

mkdir -p ${WORKSPACE}/openGaussBeta/server/binarylibs
#mkdir -p ${WORKSPACE}/openGauss/server/third_party
#wget -nH -c -r -np -k -L -p -P ${WORKSPACE} ftp://mpp:mpp@192.168.0.155/openGauss-third_party_binarylibs.tar.gz
set +x
wget -q -P ${WORKSPACE} ftp://mpp:mpp@192.168.0.155/openGauss-third_party_binarylibs.tar.gz -O ${WORKSPACE}/openGauss-third_party_binarylibs.tar.gz
set -x
tar -zxf ${WORKSPACE}/openGauss-third_party_binarylibs.tar.gz -C ${WORKSPACE}
cp -rf ${WORKSPACE}/openGauss-third_party_binarylibs/* ${WORKSPACE}/openGaussBeta/server/binarylibs
cp ${WORKSPACE}/pldebugger_3_0.tar.gz ${WORKSPACE}/openGaussBeta/server/third_party/dependency/pldebugger
#cp -rf ${WORKSPACE}/openGauss/third_party/* ${WORKSPACE}/openGaussBeta/server/third_party


# 全局变量

rm -rf openGauss.properties

pkg_type_capital=`echo ${PKG_TYPE} | tr a-z A-Z`

echo "TARGET_PKG_NAME=${PKG_PRE_FIX}_${PKG_VERSION}_PACKAGES_${pkg_type_capital}.tar.gz"  &gt; openGaussBeta.properties
echo "OUT_PUT_PATH=${WORKSPACE}/output/compile/beta" &gt;&gt; openGaussBeta.properties

</command>
    </hudson.tasks.Shell>
    <EnvInjectBuilder plugin="envinject@2.3.0">
      <info>
        <propertiesFilePath>openGaussBeta.properties</propertiesFilePath>
      </info>
    </EnvInjectBuilder>
    <hudson.tasks.Shell>
      <command>
echo "OUT_PUT_PATH:${OUT_PUT_PATH},TARGET_PKG_NAME:$TARGET_PKG_NAME"

echo "branch:${branch},pkg type:${PKG_TYPE}, OS:${TARGET_OS},version:${PKG_VERSION}"

mkdir -p ${OUT_PUT_PATH}

third_pull() {
   cd ${WORKSPACE}/openGaussBeta/server/thrid_party
   git pull origin master
   
   cd ${WORKSPACE}/openGaussBeta/server/binarylibs
   git pull origin master

}

# 开始打包
#cd ${WORKSPACE}/openGauss_CI/scripts/versionBuild/compile
#chmod +x package_ci.sh
sh -x /home/packageBeta_ci.sh -d ${WORKSPACE} -t ${PKG_TYPE} -v ${PKG_PRE_FIX}_${PKG_VERSION} -b build -o ${OUT_PUT_PATH} -l ${WORKSPACE}/openGaussBeta/binarylibs





</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.ArtifactArchiver>
      <artifacts>output/compile/release/*.log</artifacts>
      <allowEmptyArchive>false</allowEmptyArchive>
      <onlyIfSuccessful>false</onlyIfSuccessful>
      <fingerprint>false</fingerprint>
      <defaultExcludes>true</defaultExcludes>
      <caseSensitive>true</caseSensitive>
    </hudson.tasks.ArtifactArchiver>
  </publishers>
