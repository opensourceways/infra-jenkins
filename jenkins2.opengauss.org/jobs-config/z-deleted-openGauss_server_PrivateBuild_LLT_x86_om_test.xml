    <hudson.tasks.Shell>
      <command>echo "giteePullRequestIid: ${giteePullRequestIid}"
echo "giteeAfterCommitSha: ${giteeAfterCommitSha}"
echo "giteeRef: ${giteeRef}"
git config --global core.compression 0


cd ${WORKSPACE}
rm -rf openGauss-third_party_binarylibs*
rm -rf om
set +x
wget -q -P ${WORKSPACE} ftp://mpp:mpp@192.168.0.155/openGauss-third_party_binarylibs.tar.gz -O ${WORKSPACE}/openGauss-third_party_binarylibs.tar.gz
set -x
tar -zxf ${WORKSPACE}/openGauss-third_party_binarylibs.tar.gz


# 打包工程
cd ${WORKSPACE}/openGauss/build/script
chmod +x build_opengauss.sh
sed -i 's/make -sj 8/make -sj 6 /g' mpp_package.sh
sed -i 's/make install -sj 8/make install -sj 6 /g' mpp_package.sh

cd ${WORKSPACE}/openGauss
#sh build.sh -m release -3rd ${WORKSPACE}/openGauss-third_party_binarylibs -pkg

mkdir -p output
# 打包om
cd ${WORKSPACE}/OM
sh build.sh -3rd ${WORKSPACE}/openGauss-third_party_binarylibs

# copy om包
cp ${WORKSPACE}/OM/package/*om.tar.gz ${WORKSPACE}/openGauss/output/.


# 执行om
cd /home/OM

export LD_LIBRARY_PATH=/usr1/gauss_jenkins/jenkins/workspace/om/package/script/gspylib/clib:$LD_LIBRARY_PATH

# 安装
sh automated-test.sh /usr1/gauss_jenkins/jenkins/workspace

if [ -e /home/OM/ssh/known_hosts ]
then
    rm -rf ~/.ssh/*
    cp /home/OM/ssh/* ~/.ssh/
fi
</command>
