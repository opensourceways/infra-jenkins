<?xml version="1.1" encoding="UTF-8"?>
<slave>
  <name>docker-x86-slaves</name>
  <description></description>
  <remoteFS>/var/jenkins_home</remoteFS>
  <numExecutors>4</numExecutors>
  <mode>NORMAL</mode>
  <retentionStrategy class="hudson.slaves.RetentionStrategy$Always"/>
  <launcher class="hudson.plugins.sshslaves.SSHLauncher" plugin="ssh-slaves@1.31.2">
    <host>192.168.1.166</host>
    <port>22</port>
    <credentialsId>jenkins_master_ssh_key</credentialsId>
    <javaPath>/usr/bin/java</javaPath>
    <launchTimeoutSeconds>60</launchTimeoutSeconds>
    <maxNumRetries>10</maxNumRetries>
    <retryWaitTime>15</retryWaitTime>
    <sshHostKeyVerificationStrategy class="hudson.plugins.sshslaves.verifiers.NonVerifyingKeyVerificationStrategy"/>
    <tcpNoDelay>true</tcpNoDelay>
  </launcher>
  <label>docker x86</label>
  <nodeProperties/>
</slave>