<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>this project is for openGauss-webclient image build</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <com.gitee.jenkins.connection.GiteeConnectionProperty plugin="gitee@1.1.14">
      <giteeConnection>gitee</giteeConnection>
    </com.gitee.jenkins.connection.GiteeConnectionProperty>
    <com.chikli.hudson.plugin.naginator.NaginatorOptOutProperty plugin="naginator@1.18.1">
      <optOut>false</optOut>
    </com.chikli.hudson.plugin.naginator.NaginatorOptOutProperty>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>build_version</name>
          <defaultValue>v0.3.0</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>PGWEB_ADDRESS</name>
          <defaultValue>https://gitee.com/opengauss/openGauss-webclient/attach_files/1095681/download/openGauss-webclient_linux_amd64.zip</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@4.7.1">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://github.com/opensourceways/playground-images.git</url>
        <credentialsId>tommylike_github_token</credentialsId>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/main</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="empty-list"/>
    <extensions/>
  </scm>
  <assignedNode>docker</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
set -e

GIT_EMAIL=&quot;tommylike@gmail.com&quot;
#IMAGE_VERSION=$(git rev-parse HEAD)
PROJECT_NAME=&quot;opengauss-webclient&quot;
IMAGE_VERSION=$build_version

if hash docker 2&gt;/dev/null; then
    docker login -u cn-north-4@$DOCKER_USER -p $DOCKER_PASS swr.cn-north-4.myhuaweicloud.com/opensourceway/opengauss/webclient
else
	echo &quot;docker not installed.&quot;
    exit
fi


echo &quot;cd workspace&quot;
ls
cd $WORKSPACE/opengauss/opengauss-pgweb-dockerfile

cat Dockerfile

docker build --build-arg PGWEB_ADDRESS=&quot;$PGWEB_ADDRESS&quot; -t swr.cn-north-4.myhuaweicloud.com/opensourceway/opengauss/webclient:$IMAGE_VERSION .
docker push     swr.cn-north-4.myhuaweicloud.com/opensourceway/opengauss/webclient:$IMAGE_VERSION

docker rmi      swr.cn-north-4.myhuaweicloud.com/opensourceway/opengauss/webclient:$IMAGE_VERSION</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.plugins.ws__cleanup.WsCleanup plugin="ws-cleanup@0.39">
      <patterns class="empty-list"/>
      <deleteDirs>false</deleteDirs>
      <skipWhenFailed>false</skipWhenFailed>
      <cleanWhenSuccess>true</cleanWhenSuccess>
      <cleanWhenUnstable>true</cleanWhenUnstable>
      <cleanWhenFailure>true</cleanWhenFailure>
      <cleanWhenNotBuilt>true</cleanWhenNotBuilt>
      <cleanWhenAborted>true</cleanWhenAborted>
      <notFailBuild>false</notFailBuild>
      <cleanupMatrixParent>false</cleanupMatrixParent>
      <externalDelete></externalDelete>
      <disableDeferredWipeout>false</disableDeferredWipeout>
    </hudson.plugins.ws__cleanup.WsCleanup>
  </publishers>
  <buildWrappers>
    <org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper plugin="credentials-binding@1.27">
      <bindings>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>huaweicloud-swr-token</credentialsId>
          <usernameVariable>DOCKER_USER</usernameVariable>
          <passwordVariable>DOCKER_PASS</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>tommylike_github_token</credentialsId>
          <usernameVariable>GIT_USER</usernameVariable>
          <passwordVariable>GIT_PASS</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>00441642-0437-4b00-9be4-ec0bb484d5c1</credentialsId>
          <usernameVariable>ARGOCD_USER</usernameVariable>
          <passwordVariable>ARGOCD_PASS</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
      </bindings>
    </org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper>
  </buildWrappers>
</project>