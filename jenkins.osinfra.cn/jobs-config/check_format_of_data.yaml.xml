  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
set -e

if [ -d &quot;${REPO_NAME}&quot; ]; then
    rm -rf &quot;${REPO_NAME}&quot;
fi

git clone https://gitee.com/${REPO_OWNER}/${REPO_NAME}.git
cd ${REPO_NAME}
git fetch origin pull/${PULL_NUMBER}/head:master-${PULL_NUMBER}
git checkout master-${PULL_NUMBER}



cd ..
if [ -d &quot;om-data&quot; ]; then
    rm -rf &quot;om-data&quot;
fi
git clone https://gitee.com/opensourceway/om-data.git
cd om-data/src
python3 check_yaml.py  -p ../../${REPO_NAME}/om-data/data.yaml


</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
