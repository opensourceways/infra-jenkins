<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>The project is used to update mindspore prow bazel</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <com.gitee.jenkins.connection.GiteeConnectionProperty plugin="gitee@1.1.14">
      <giteeConnection>gitee</giteeConnection>
    </com.gitee.jenkins.connection.GiteeConnectionProperty>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>VERSION</name>
          <defaultValue>master</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@4.7.1">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://github.com/opensourceways/test-infra.git</url>
        <credentialsId>tommylike_github_token</credentialsId>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/sync-5-22</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="empty-list"/>
    <extensions/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
set -e
IMAGE_VERSOPM=&quot;v0.0.1&quot;
GIT_EMAIL=&quot;tommylike@gmail.com&quot;

if hash git 2&gt;/dev/null; then
	IMAGE_VERSION=$(git rev-parse HEAD)
fi

if hash docker 2&gt;/dev/null; then
	docker login -u ap-southeast-1@$DOCKER_USER -p $DOCKER_PASS swr.ap-southeast-1.myhuaweicloud.com
else
	echo &quot;docker not installed&quot;
fi
cd $WORKSPACE
docker build -f prow/Dockerfile.base -t swr.ap-southeast-1.myhuaweicloud.com/opensourceway/prow/bazel2-env:latest .
docker tag swr.ap-southeast-1.myhuaweicloud.com/opensourceway/prow/bazel2-env:latest swr.ap-southeast-1.myhuaweicloud.com/opensourceway/prow/bazel2-env:$IMAGE_VERSION
#docker push swr.ap-southeast-1.myhuaweicloud.com/opensourceway/prow/bazel2-env:latest
#docker push swr.ap-southeast-1.myhuaweicloud.com/opensourceway/prow/bazel2-env:$IMAGE_VERSION
docker rmi swr.ap-southeast-1.myhuaweicloud.com/opensourceway/prow/bazel2-env:latest
docker rmi swr.ap-southeast-1.myhuaweicloud.com/opensourceway/prow/bazel2-env:$IMAGE_VERSION





</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <org.jvnet.hudson.plugins.groovypostbuild.GroovyPostbuildRecorder plugin="groovy-postbuild@2.5">
      <script plugin="script-security@1.76">
        <script>manager.addShortText(manager.build.buildVariables.get(&quot;VERSION&quot;))</script>
        <sandbox>false</sandbox>
      </script>
      <behavior>0</behavior>
      <runForMatrixParent>false</runForMatrixParent>
    </org.jvnet.hudson.plugins.groovypostbuild.GroovyPostbuildRecorder>
  </publishers>
  <buildWrappers>
    <org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper plugin="credentials-binding@1.23">
      <bindings>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>huaweicloud-swr-token</credentialsId>
          <usernameVariable>DOCKER_USER</usernameVariable>
          <passwordVariable>DOCKER_PASS</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>tommylike_github_token</credentialsId>
          <usernameVariable>GIT_USER</usernameVariable>
          <passwordVariable>GIT_PASS</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
      </bindings>
    </org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper>
  </buildWrappers>
</project>