<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>This project used to publish mindspore website test with rasp for  tutorilas_developer branch;</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <com.gitee.jenkins.connection.GiteeConnectionProperty plugin="gitee@1.1.14">
      <giteeConnection>gitee</giteeConnection>
    </com.gitee.jenkins.connection.GiteeConnectionProperty>
  </properties>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@4.7.1">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://gitee.com/mindspore/mindspore.github.io.git</url>
        <credentialsId>tommylike_gitee_username_password</credentialsId>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/tutorials-develop</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="empty-list"/>
    <extensions/>
  </scm>
  <assignedNode>docker</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
set -e

GIT_EMAIL=&quot;tommylike@gmail.com&quot;
#IMAGE_VERSION=$(git rev-parse HEAD)
IMAGE_VERSION=npm_ci_0.0.1
PROJECT_NAME=&quot;mindspore-test-website&quot;
GITEE_EMAIL=&quot;yang_liu0418@163.com&quot;

if hash docker 2&gt;/dev/null; then
    docker login -u ap-southeast-1@$DOCKER_USER -p $DOCKER_PASS swr.ap-southeast-1.myhuaweicloud.com
else
	echo &quot;docker not installed.&quot;
    exit
fi

# get rasp.tgz/setenv.sh
if  [ -d &quot;mindspore_rasp_test&quot; ]; then
    rm -rf &quot;mindspore_rasp_test&quot;
fi

git config --global user.email $GITEE_EMAIL
git config --global user.name  $GITEE_LIUYANG

git clone https://liuyanglinux:Liu19960418@gitee.com/liuyanglinux/mindspore_rasp_test
cp mindspore_rasp_test/rasp.tgz mindspore_rasp_test/setenv.sh $WORKSPACE

cd $WORKSPACE

#sed -i &apos;16a RUN npm install -g node@16 &amp;&amp; npm install -g npm@8.5.1&apos; ./Dockerfile
#sed -i &quot;s|registry.npmmirror.com|repo.huaweicloud.com/repository/npm/|&quot; ./Dockerfile
sed -i &quot;s|npm install --registry=https://registry.npmmirror.com|npm ci --registry=https://registry.npm.taobao.org|&quot; ./Dockerfile

# add to deploy rasp
sed -i &apos;36a ADD rasp.tgz \/path\/to\/app_home\/&apos; ./Dockerfile
sed -i &apos;37a COPY setenv.sh /usr/local/tomcat/bin/&apos; ./Dockerfile
sed -i &apos;38a RUN chown -R root\:root \/path\/to\/app_home\/rasp &amp;&amp; chmod 755 -R \/path\/to\/app_home\/rasp&apos; ./Dockerfile
sed -i &apos;39a RUN chmod 750 \/usr\/local\/tomcat\/bin\/setenv.sh&apos; ./Dockerfile

cat Dockerfile

docker build -t swr.ap-southeast-1.myhuaweicloud.com/opensourceway/mindspore-website-test:$IMAGE_VERSION .
docker push     swr.ap-southeast-1.myhuaweicloud.com/opensourceway/mindspore-website-test:$IMAGE_VERSION

docker rmi      swr.ap-southeast-1.myhuaweicloud.com/opensourceway/mindspore-website-test:$IMAGE_VERSION


</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.Mailer plugin="mailer@1.34">
      <recipients>zhaochunjiang1@huawei.com kathy.wangting@huawei.com</recipients>
      <dontNotifyEveryUnstableBuild>false</dontNotifyEveryUnstableBuild>
      <sendToIndividuals>true</sendToIndividuals>
    </hudson.tasks.Mailer>
    <hudson.plugins.ws__cleanup.WsCleanup plugin="ws-cleanup@0.39">
      <patterns class="empty-list"/>
      <deleteDirs>false</deleteDirs>
      <skipWhenFailed>false</skipWhenFailed>
      <cleanWhenSuccess>true</cleanWhenSuccess>
      <cleanWhenUnstable>true</cleanWhenUnstable>
      <cleanWhenFailure>true</cleanWhenFailure>
      <cleanWhenNotBuilt>true</cleanWhenNotBuilt>
      <cleanWhenAborted>true</cleanWhenAborted>
      <notFailBuild>false</notFailBuild>
      <cleanupMatrixParent>false</cleanupMatrixParent>
      <externalDelete></externalDelete>
      <disableDeferredWipeout>false</disableDeferredWipeout>
    </hudson.plugins.ws__cleanup.WsCleanup>
  </publishers>
  <buildWrappers>
