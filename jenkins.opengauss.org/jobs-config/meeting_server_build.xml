  <builders>
    <hudson.tasks.Shell>
      <command># https://github.com/opensourceways/app-meeting-server.git  opengauss  openGauss-meeting
#
#
cd $WORKSPACE
echo "###### build repo ######"

echo "build repo is: meeting-server: $build_server"

time=`date "+%Y%m%d%H%M%S"`
build_images="v1.0.$time"

echo "build version: $build_images"

meeting_imageId=""


if [ "$build_server" == "true" ]; then
	project_name="meeting-server"
	echo "###################################### build meeting-server: $build_images ######################################"
    cd "$WORKSPACE/openGauss-meeting"
    docker build -t ${project_name}:${build_images} .
    imageId=$(docker images | grep ${project_name} | grep ${build_images} | awk '{print $3}')
    sh /var/jenkins_home/swr_upload.sh opengauss  ${project_name}:${build_images}
    echo "build meeting-server successful; $imageId"
fi


if [ $imageId ]; then
	echo "remote meeting images: $imageId"
    docker rmi -f $imageId
fi


echo "build task end."

</command>
    </hudson.tasks.Shell>
