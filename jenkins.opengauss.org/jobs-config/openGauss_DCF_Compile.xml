      <command>echo "giteePullRequestIid: ${giteePullRequestIid}"
echo "giteeAfterCommitSha: ${giteeAfterCommitSha}"
echo "giteeRef: ${giteeRef}"
git config --global core.compression 0

DCF_GITEE_REPO=https://gitee.com/opengauss/DCF.git

function install_cmake()
{
    ###yum install openssl-devel -y
    source_path=/opt/3rdbuild/cmake
    target_path=/usr/local/cmake318
    mkdir -p ${source_path}
    cd $source_path

    wget "https://gcc-zz.obs.ap-southeast-1.myhuaweicloud.com/cmake-3.18.5.tar.gz"
    tar -zxf cmake-3.18.5.tar.gz
    cd cmake-3.18.5

    ./configure --prefix=${target_path}
    make -sj8
    make install -sj8
}

#if hash cmake 2&gt;/dev/null; then
#    echo "cmake version is $(cmake --version)"
#else
    #install_cmake#
#fi

a=0
flag=0
while [ $a -lt 10 ]
do
   echo $a
   rm -rf ${WORKSPACE}/DCF
   timeout 60 git clone -b ${giteeTargetBranch} "${DCF_GITEE_REPO}" "${WORKSPACE}/DCF"
   if [[ $? == 0 ]] ; then
      flag=1
      break
   fi
   a=`expr $a + 1`
done

if [[ $flag == 0 ]] ; then
      echo "clone code failed!"
      exit 1
fi


cd ${WORKSPACE}/DCF
git rev-parse --is-inside-work-tree
git config remote.origin.url ${DCF_GITEE_REPO}
git fetch --tags --force --progress origin ${giteeRef}:${giteeRef}
git checkout -b ${giteeRef} ${giteeRef}


mkdir -p ${WORKSPACE}/openGauss/binarylibs
cd ${WORKSPACE}
rm -rf openGauss-third_party_binarylibs*
set +x
wget -q -P ${WORKSPACE} ftp://mpp:openGauss1%232_3%24@192.168.0.155/openGauss-third_party_binarylibs.tar.gz -O ${WORKSPACE}/openGauss-third_party_binarylibs.tar.gz
set -x
tar -zxf ${WORKSPACE}/openGauss-third_party_binarylibs.tar.gz

echo "Start to build DCF..."
# 构建DCF
cd ${WORKSPACE}/DCF/build/linux/opengauss
platform=$(sh get_platform_str.sh)
export PLAT_FORM_STR=${platform}
sh build.sh -3rd ${WORKSPACE}/openGauss-third_party_binarylibs -m Release -t cmake

if [[ $? == 0 ]] ; then
    echo "openGauss-DCF build success."
else
    echo "openGauss-DCF build failed."
    exit 1
fi


</command>
