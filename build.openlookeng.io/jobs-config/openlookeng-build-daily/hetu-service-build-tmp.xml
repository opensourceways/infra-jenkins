      <defaultExcludes>true</defaultExcludes>
      <caseSensitive>true</caseSensitive>
      <followSymlinks>false</followSymlinks>
    </hudson.tasks.ArtifactArchiver>
    <hudson.plugins.jacoco.JacocoPublisher plugin="jacoco@3.1.1">
      <execPattern>hetu-**/target/**.exec</execPattern>
      <classPattern>hetu-**/target/classes</classPattern>
      <sourcePattern>hetu-**/src/main/java </sourcePattern>
      <sourceInclusionPattern>**/*.java</sourceInclusionPattern>
      <sourceExclusionPattern></sourceExclusionPattern>
      <inclusionPattern></inclusionPattern>
      <exclusionPattern></exclusionPattern>
      <skipCopyOfSrcFiles>false</skipCopyOfSrcFiles>
