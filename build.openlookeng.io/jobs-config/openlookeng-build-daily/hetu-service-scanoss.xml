a=`cat $FILE_PATH|cut -d &apos;&quot;&apos; -f12`
if [ $a = &apos;failed&apos; ];then
((result++));
elif [ $a = &apos;error&apos; ];then
((result++));
fi

done
if [ $result -gt 0 ];then
echo &apos;scanoss no pass!&apos;
exit 1;
fi
</command>
