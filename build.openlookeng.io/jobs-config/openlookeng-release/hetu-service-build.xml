    <hudson.tasks.Shell>
      <command>source /home/openlookeng/.bashrc

clone_code() {
    local url=$1
    local repo=$2    
    local branch=$3
    local msg=&quot;&quot;

    test -n &quot;$branch&quot; &amp;&amp; branch=&quot;-b $branch&quot;

    for i in {1..3}; do
        msg=$(git clone $branch $url)
        test $? -eq 0 &amp;&amp; return 0

        sleep 1
        rm -fr $repo
    done

    echo $msg
    return 1
}
clone_code git@gitee.com:openlookeng/hetu-maven-plugin.git hetu-maven-plugin
