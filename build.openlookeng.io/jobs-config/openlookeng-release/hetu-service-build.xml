    <hudson.tasks.Shell>
      <command>source /home/openlookeng/.bashrc

whoami
ulimit -a
mvn --version
java -version

rm -rf /tmp/* /tmp/*.* || true

# remove all containers
docker ps -aq | tr &apos;\n&apos; &apos; &apos; | xargs -rI % sh -c &apos;docker rm -f %;&apos;
sleep 15

cd hetu-core

mvn clean install -DskipTests -T 2C
#mvn test -pl &apos;!hetu-server,!hetu-server-rpm&apos;

hetujdbcpakname=$(ls presto-jdbc/target/ | grep -v “original-*” | grep -oP &quot;hetu-jdbc-\d+.\d+.\d+-?(SNAPSHOT)?.jar&quot; | uniq)

#cd hetu-jdbc/target/ &amp;&amp; ls | grep -v ^${hetujdbcpakname} | grep jar | xargs rm -f
#cd -

mkdir -p ${version}
cp hetu-server/target/hetu-server-*.tar.gz ${version}/
cp hetu-server-rpm/target/hetu-server-rpm-*.rpm ${version}/
cp presto-cli/target/hetu-cli-*executable.jar ${version}/
cp presto-jdbc/target/${hetujdbcpakname} ${version}/
cp hetu-sql-migration-tool/target/hetu-sql-migration-cli-*executable.jar ${version}/

cd ${version} &amp;&amp; paklist=$(ls)

for i in ${paklist}
do
	sha256sum ${i} &gt; ${i}.sha256sum
done

if [[ &quot;$isReleaseVersion&quot; == &quot;True&quot; ]]
then
	cd -
	scp -i /home/openlookeng/.ssh/rsa.key -P 30322 -r ${version}/ root@10.0.0.191:/repo/openlookeng/
	
fi

