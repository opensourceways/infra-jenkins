  <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@2.92">
    <script>pipeline {

    agent {
		node { 
			label &quot;vm-slave-scan&quot; 
		}
	}

    stages {

    stage(&apos;codecheck&apos;) {
        
        steps {
            
            withCredentials([string(credentialsId: &apos;codecheck_static_key&apos;, variable: &apos;codecheck_static_key&apos;)])
            
            {
                script {

                    //set SECRET with the credential content
        
                    //def pr_url = &quot;${giteeTargetRepoHttpUrl}/${REPO_OWNER}/${REPO_NAME}/pulls/${PULL_NUMBER}&quot;
                    pr_url = giteeTargetRepoHttpUrl + &quot;/&quot; + REPO_OWNER + &quot;/&quot; + REPO_NAME + &quot;/pulls/&quot; + PULL_NUMBER
                    
                    echo &quot;${pr_url}&quot;

                    //sh &apos;source /opt/codecheck/.env/bin/activate&apos;

                    sh &quot;python3 /opt/codecheck/codecheck.py --pr_url &apos;${pr_url}&apos; --static_key ${codecheck_static_key}&quot;
                }
            }
        }

    }

  }

}
</script>
    <sandbox>true</sandbox>
  </definition>
  <triggers/>
  <authToken>11cb8527fb1fa37227673a0aa87ad52891</authToken>
  <disabled>false</disabled>
