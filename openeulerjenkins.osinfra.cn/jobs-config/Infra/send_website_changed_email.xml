  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/sh -e

test -f send_changed_email.py &amp;&amp; rm -f send_changed_email.py
test -f config.yaml &amp;&amp; rm -f config.yaml
test -f watch_repo_pr.yaml &amp;&amp; rm -f watch_repo_pr.yaml
wget https://gitee.com/openeuler/infrastructure/raw/master/ci/tools/send_note_email/send_changed_email.py
wget https://gitee.com/openeuler/infrastructure/raw/master/ci/tools/send_note_email/config.yaml
cp config.yaml watch_repo_pr.yaml
python3 send_changed_email.py -o $giteeTargetNamespace -r $giteeTargetRepoName -n $giteePullRequestIid -t $GITEE_TOKEN -s $SMTP_HOST -p $SMTP_PORT -u $SMTP_USER -pw $SMTP_PASS</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
