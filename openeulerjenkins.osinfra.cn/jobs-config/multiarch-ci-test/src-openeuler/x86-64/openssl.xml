    <hudson.tasks.Shell>
      <command>#!/bin/bash
repo_owner=&apos;fan-xiaoyaya&apos;
# 调测可使用以下代码
JENKINS_HOME=/home/jenkins/oecp
RPM_PACKAGE=/home/jenkins/agent/workspace/multiarch-ci-test/src-openeuler/${arch}/openssl
mkdir -p ${JENKINS_HOME}
mkdir -p ${RPM_PACKAGE}
echo ${MysqlUserPasswd}
echo ${MysqldbHost}
echo ${MysqldbPort}

rm -rf ${JENKINS_HOME}
git clone -b br_symbol https://15336166610:P4ssW0rd@gitee.com/openeuler-customization/oecp.git ${JENKINS_HOME}
sed -i &quot;s/dbhost=127.0.0.1/dbhost=114.116.251.180/g&quot; ${JENKINS_HOME}/oecp/conf/oecp.conf 
sed -i &quot;s/dbpassword=/dbpassword=${MysqlUserPasswd:5}/g&quot; ${JENKINS_HOME}/oecp/conf/oecp.conf
sed -i &quot;s/dbport=3306/dbport=30250/g&quot; ${JENKINS_HOME}/oecp/conf/oecp.conf
cat ${JENKINS_HOME}/oecp/conf/oecp.conf

rm -rf ${RPM_PACKAGE}/*
#wget -r -np -nH -R index.html http://119.3.219.20:82/openEuler:/Epol/standard_${arch}/${arch}/KubeOS-1.0.1-5.oe1.aarch64.rpm -P ${RPM_PACKAGE}
wget -r -np -nH -R index.html http://119.3.219.20:82/openEuler:/Mainline/standard_${arch}/${arch}/openssl-perl-1.1.1m-5.oe1.x86_64.rpm   -P ${RPM_PACKAGE}
wget -r -np -nH -R index.html http://119.3.219.20:82/openEuler:/Mainline/standard_${arch}/${arch}/openssl-devel-1.1.1m-5.oe1.x86_64.rpm   -P ${RPM_PACKAGE}
wget -r -np -nH -R index.html http://119.3.219.20:82/openEuler:/Mainline/standard_${arch}/${arch}/openssl-libs-1.1.1m-5.oe1.x86_64.rpm   -P ${RPM_PACKAGE}


pip3 install -i https://repo.huaweicloud.com/repository/pypi/simple -r ${JENKINS_HOME}/requirement
python3 ${JENKINS_HOME}/cli.py --db-password ${MysqlUserPasswd:5} -s master_${arch} ${RPM_PACKAGE}

set +x
set -e
#main
set +e
set -x</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
