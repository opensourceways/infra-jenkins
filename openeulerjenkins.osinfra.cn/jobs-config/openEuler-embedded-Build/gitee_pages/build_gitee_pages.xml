        </org.jenkinsci.plugins.gwt.GenericVariable>
        <org.jenkinsci.plugins.gwt.GenericVariable>
          <expressionType>JSONPath</expressionType>
          <key>giteeTargetNamespace</key>
          <value>pull_request.base.repo.namespace</value>
          <regexpFilter></regexpFilter>
          <defaultValue></defaultValue>
        </org.jenkinsci.plugins.gwt.GenericVariable>
        <org.jenkinsci.plugins.gwt.GenericVariable>
          <expressionType>JSONPath</expressionType>
          <key>giteeCommitter</key>
          <value>pull_request.user.login</value>
          <regexpFilter></regexpFilter>
          <defaultValue></defaultValue>
        </org.jenkinsci.plugins.gwt.GenericVariable>
        <org.jenkinsci.plugins.gwt.GenericVariable>
          <expressionType>JSONPath</expressionType>
          <key>comment</key>
          <value>comment.body</value>
          <regexpFilter></regexpFilter>
          <defaultValue></defaultValue>
        </org.jenkinsci.plugins.gwt.GenericVariable>
        <org.jenkinsci.plugins.gwt.GenericVariable>
          <expressionType>JSONPath</expressionType>
          <key>commentID</key>
          <value>comment.id</value>
          <regexpFilter></regexpFilter>
          <defaultValue></defaultValue>
        </org.jenkinsci.plugins.gwt.GenericVariable>
        <org.jenkinsci.plugins.gwt.GenericVariable>
          <expressionType>JSONPath</expressionType>
          <key>jobTriggerTime</key>
          <value>comment.updated_at</value>
          <regexpFilter></regexpFilter>
          <defaultValue></defaultValue>
        </org.jenkinsci.plugins.gwt.GenericVariable>
        <org.jenkinsci.plugins.gwt.GenericVariable>
          <expressionType>JSONPath</expressionType>
          <key>prCreateTime</key>
          <value>pull_request.created_at</value>
          <regexpFilter></regexpFilter>
          <defaultValue></defaultValue>
        </org.jenkinsci.plugins.gwt.GenericVariable>
        <org.jenkinsci.plugins.gwt.GenericVariable>
          <expressionType>JSONPath</expressionType>
          <key>triggerLink</key>
          <value>comment.html_url</value>
          <regexpFilter></regexpFilter>
          <defaultValue></defaultValue>
        </org.jenkinsci.plugins.gwt.GenericVariable>
      </genericVariables>
      <regexpFilterText>$giteeTargetNamespace $giteeRepoName $commentEOF</regexpFilterText>
      <regexpFilterExpression>openeuler yocto-meta-openeuler ([\s\S]Hi[\s\S]*|/build_docs)EOF</regexpFilterExpression>
      <printPostContent>false</printPostContent>
      <printContributedVariables>false</printContributedVariables>
      <causeString>$giteeRepoName trigger by pull request $giteePullRequestIid  [$giteeSourceNamespace:$giteeSourceBranch -&gt; $giteeTargetNamespace:$giteeTargetBranch]</causeString>
      <token></token>
      <silentResponse>false</silentResponse>
      <overrideQuietPeriod>false</overrideQuietPeriod>
    </org.jenkinsci.plugins.gwt.GenericTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>pwd &amp;&amp; ls *

comment_url=https://gitee.com/api/v5/repos/openeuler/yocto-meta-openeuler/pulls/${giteePullRequestIid}/comments
# 安装diff工具
sudo yum -y install diffutils

# 检查是否为master分支，如果不是，则跳过
echo &quot;目标分支：${giteeTargetBranch}&quot;
if [ ${giteeTargetBranch} != &quot;master&quot; ]; then
    curl -X POST --header &apos;Content-Type: application/json;charset=UTF-8&apos; $comment_url -d &apos;{&quot;access_token&quot;:&quot;d46d69246296c81399f1ae26eda67f90&quot;,&quot;body&quot;:&quot;Sphinx工程：当前仅检查master分支docs文件变更! &apos;${BUILD_URL}&apos;&quot;}&apos;
    exit 0
fi

# 检查是否涉及docs目录文件修改，如果不涉及，则跳过
git clone https://harvey-rtos:Ruljw*923QA@gitee.com/openeuler/yocto-meta-openeuler.git -b master -v docs_build_work
res=`diff -ruNaq -x &quot;.git&quot; ${giteeRepoName} docs_build_work | grep &quot;docs_build_work/docs/&quot; || echo &quot;&quot;`
if [ &quot;$res&quot; == &quot;&quot; ]; then
    echo &quot;No file modification in docs directory&quot;
    exit 0
fi

# 检查是否包含docs以外的代码修改，如果是，则失败退出
files=`diff -ruNaq -x &quot;.git&quot; ${giteeRepoName} docs_build_work | grep -v &quot;docs_build_work/docs/&quot; || echo &quot;&quot;`
if [ &quot;$files&quot; != &quot;&quot; ]; then
    echo &quot;不允许docs部分文件与其他代码同时提交！&quot;
    curl -X POST --header &apos;Content-Type: application/json;charset=UTF-8&apos; $comment_url -d &apos;{&quot;access_token&quot;:&quot;d46d69246296c81399f1ae26eda67f90&quot;,&quot;body&quot;:&quot;Sphinx工程：当前PR中包含docs目录之外的代码变更!  &apos;${BUILD_URL}&apos;&quot;}&apos;
    exit 1
fi

# 安装构建工具
sudo yum -y install make python-sphinx
sudo wget https://bootstrap.pypa.io/get-pip.py
python3 get-pip.py
pip3 install sphinx
pip3 install sphinx_rtd_theme

export PATH=/home/huawei/.local/:$PATH

# 执行编译工程
pushd ${giteeRepoName}/docs
make html
if [ $? != 0 ]; then
    echo &quot;构建失败！&quot;
    curl -X POST --header &apos;Content-Type: application/json;charset=UTF-8&apos; $comment_url -d &apos;{&quot;access_token&quot;:&quot;d46d69246296c81399f1ae26eda67f90&quot;,&quot;body&quot;:&quot;Sphinx工程：build failed!  &apos;${BUILD_URL}&apos;&quot;}&apos;
    exit 1
fi
popd
curl -X POST --header &apos;Content-Type: application/json;charset=UTF-8&apos; $comment_url -d &apos;{&quot;access_token&quot;:&quot;d46d69246296c81399f1ae26eda67f90&quot;,&quot;body&quot;:&quot;Sphinx工程：build success! &apos;${BUILD_URL}&apos;&quot;}&apos;


exit 0
</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
