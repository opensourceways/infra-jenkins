  <builders>
    <hudson.tasks.Shell>
      <command># 安装构建工具
sudo yum -y install make python-sphinx
sudo wget --no-check-certificate https://bootstrap.pypa.io/get-pip.py
python3 get-pip.py
pip3 install sphinx
pip3 install sphinx_rtd_theme

export PATH=/home/huawei/.local/:$PATH

# 执行编译工程
git clone https://openeuler-ci-bot:${GiteeToken}@gitee.com/openeuler/yocto-meta-openeuler.git -b master -v docs_build_work
# pushd ${giteeTargetRepoName}/docs
pushd docs_build_work/docs
make html
if [ $? != 0 ]; then
    echo &quot;构建失败！&quot;
    exit 1
fi
popd

# 上传编译结果，更新gitee_pages分支
git clone https://openeuler-ci-bot:${GiteeToken}@gitee.com/openeuler/yocto-meta-openeuler.git -b gitee_pages -v docs_upload_work
git config --global user.email &quot;zhangweigang6@huawei.com&quot;
git config --global user.name &quot;zhangweigang&quot;
pushd docs_upload_work/docs/build
rm -rf html
cp -r ${WORKSPACE}/docs_build_work/docs/build/html ./
git status | grep &quot;html&quot; || exit 0
git add html
git commit -m &quot;update docs from ${giteePullRequestIid}&quot;
git push
if [ $? != 0 ]; then
    echo &quot;上传失败！&quot;
    exit 1
fi

exit 0
</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
