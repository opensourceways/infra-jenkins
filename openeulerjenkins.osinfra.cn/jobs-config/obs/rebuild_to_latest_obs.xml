  <builders>
    <hudson.tasks.Shell>
      <command>set +e
rm -rf openeuler_obs-*
set -e

if false;then
rm -rf obs_meta
for((i=1;i&lt;=5;i++));
do
	set +e
	git clone https://$GiteeCloneUserName:$GiteeClonePassword@gitee.com/src-openeuler/obs_meta.git
    set -e
    if [ -d obs_meta ];then
       		break
    fi
done
fi
sudo su - root &lt;&lt;EOF
git config --global http.postBuffer 52428800
git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;
git config --global pull.ff only
touch ~/.oscrc
cat &gt; ~/.oscrc &lt;&lt; FEOF
[general]
apiurl = http://117.78.1.88
[http://117.78.1.88]
user = $OBS_USER
pass = $OBS_PWD
FEOF

wget http://172.16.1.95:88/mkb/openeuler-obs/openeuler_obs-v1.4.4-aarch64.tar.gz
tar -xvf openeuler_obs-v1.4.4-aarch64.tar.gz &amp;&amp; cd openeuler_obs-v1.4.4-aarch64

#./openeuler_obs  -sgo true -o ../obs_meta -b ${branch} -p ${obs_project_name} -ip ${OBS_SOURCE_IP} -suser root -spwd ${OBS_PASSWORD} -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword} --pkglist ${packages}
./openeuler_obs  -sgo true  -b ${branch} -p ${obs_project_name} -ip ${OBS_SOURCE_IP} -suser root -spwd ${OBS_PASSWORD} -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword} --pkglist ${packages}
EOF
</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
