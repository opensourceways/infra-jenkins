        <numToKeep>1500</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>obs_project_name</name>
          <description>obs 项目名称</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>openEuler:Factory</string>
              <string>openEuler:Mainline</string>
              <string>openEuler:Epol</string>
              <string>openEuler:20.03:LTS:SP1</string>
              <string>openEuler:20.03:LTS:SP1:Epol</string>
              <string>openEuler:Mainline:RISC-V</string>
              <string>openEuler:20.03:LTS:SP3</string>
              <string>openEuler:20.03:LTS:SP3:Epol</string>
              <string>openEuler:22.03:LTS:Next</string>
              <string>openEuler:22.03:LTS:Next:Epol</string>
              <string>openEuler:22.03:LTS</string>
              <string>openEuler:22.03:LTS:Epol</string>
              <string>openEuler:22.03:LTS:LoongArch</string>
              <string>openEuler:22.09</string>
              <string>openEuler:22.09:Epol</string>
              <string>openEuler:20.03:LTS:SP3:oepkg:openstack:common</string>
              <string>openEuler:20.03:LTS:SP3:oepkg:openstack:queens</string>
              <string>openEuler:20.03:LTS:SP3:oepkg:openstack:rocky</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>repo</name>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>standard_aarch64</string>
              <string>standard_x86_64</string>
              <string>standard_riscv64</string>
              <string>standard_loongarch64</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>pkgs</name>
          <description>若为空即全量归档。
单包或多包同步，请参照如下格式：
package1
pacakge2
package3
...</description>
          <trim>false</trim>
        </hudson.model.TextParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>OBS_BACKEND_SERVER_IP</name>
          <defaultValue>172.16.1.95</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>isbuild</name>
          <description>是否构建</description>
          <defaultValue>1</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>all</name>
          <description>是否全量归档
False：否   True：是
</description>
          <defaultValue>False</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.StringParameterDefinition>
          <name>compare</name>
          <description>差异归档
False：否   True：是
</description>
          <defaultValue>True</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>rebuild_all</name>
          <description>是否全量编译obs工程
False：否   True：是
</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>False</string>
              <string>True</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
