#fi

if [ -z $pkgs ];then
		./openeuler_obs -up True -a $all -cmp $compare -p ${obs_project_name} -repo $repo -arch ${repo##standard_} -rsip ${OBS_BACKEND_SERVER_IP} -rsu ${OBS_BACKEND_USR} -rsup ${OBS_BACKEND_PWD} -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword}
else
   		./openeuler_obs -up True -a $all -cmp $compare -p ${obs_project_name} -repo $repo -arch ${repo##standard_} -rsip ${OBS_BACKEND_SERVER_IP} -rsu ${OBS_BACKEND_USR} -rsup ${OBS_BACKEND_PWD} -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword} --pkglist $pkgs
fi
datestr=`date +&quot;%Y-%m-%d %H:%M:%S&quot;`

set -e


</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
