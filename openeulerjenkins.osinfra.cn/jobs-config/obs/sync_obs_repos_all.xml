    echo &quot;124&quot;
elif [ &quot;${OBS_BACKEND_SERVER_IP}&quot; == &quot;172.16.1.155&quot; ];then
    OBS_BACKEND_USR=${OBS_BACKEND_USR_002}
    OBS_BACKEND_PWD=${OBS_BACKEND_PWD_002}
    echo &quot;155&quot;
elif [ &quot;${OBS_BACKEND_SERVER_IP}&quot; == &quot;172.16.1.32&quot; ];then
    OBS_BACKEND_USR=${OBS_BACKEND_USR_003}
