    <hudson.tasks.Shell>
      <command>sudo su - root &lt;&lt;FEOF
echo &quot;nameserver 114.114.114.114&quot; &gt;&gt; /etc/resolv.conf
FEOF

if [[ &quot;${isbuild}&quot; == &quot;0&quot; ]];then
	exit 0
fi

(
cat &lt;&lt;EOF
[general]
apiurl=http://117.78.1.88
no_verify=yes
[http://117.78.1.88]
user=${OBS_USER}
pass=${OBS_PWD}
EOF
) &gt; ~/.oscrc


set +e
git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;
git config --global pull.ff only
rm -rf openeuler_obs-*


if [ &quot;${OBS_BACKEND_SERVER_IP}&quot; == &quot;172.16.1.95&quot; ];then
    OBS_BACKEND_USR=${OBS_BACKEND_USR_main}
    OBS_BACKEND_PWD=${OBS_BACKEND_PWD_main}
