fi

if [[ &quot;${OBS_BACKEND_SERVER_IP}&quot; == &quot;&quot; ]];then
	echo &quot;OBS_BACKEND_SERVER_IP is empty!&quot;
    exit 1
fi

if [ &quot;${OBS_BACKEND_SERVER_IP}&quot; == &quot;&quot; ];then
	echo &quot;OBS_BACKEND_SERVER_IP is empty!&quot;
    exit 1
elif [ &quot;${OBS_BACKEND_SERVER_IP}&quot; == &quot;172.16.1.95&quot; ];then
    OBS_BACKEND_USR=${OBS_BACKEND_USR_main}
    OBS_BACKEND_PWD=${OBS_BACKEND_PWD_main}
    echo &quot;95&quot;
elif [ &quot;${OBS_BACKEND_SERVER_IP}&quot; == &quot;172.16.1.124&quot; ];then
    OBS_BACKEND_USR=${OBS_BACKEND_USR_other}
    OBS_BACKEND_PWD=${OBS_BACKEND_PWD_other}
    echo &quot;124&quot;
elif [ &quot;${OBS_BACKEND_SERVER_IP}&quot; == &quot;172.16.1.155&quot; ];then
    OBS_BACKEND_USR=${OBS_BACKEND_USR_002}
    OBS_BACKEND_PWD=${OBS_BACKEND_PWD_002}
