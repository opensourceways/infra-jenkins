elif [ &quot;${OBS_BACKEND_SERVER_IP}&quot; == &quot;172.16.1.32&quot; ];then
    OBS_BACKEND_USR=${OBS_BACKEND_USR_003}
    OBS_BACKEND_PWD=${OBS_BACKEND_PWD_003}
    echo &quot;32&quot;
elif [ &quot;${OBS_BACKEND_SERVER_IP}&quot; == &quot;172.16.1.234&quot; ];then
    OBS_BACKEND_USR=${OBS_BACKEND_USR_004}
    OBS_BACKEND_PWD=${OBS_BACKEND_PWD_004}
    echo &quot;234&quot;
fi

rm -rf openeuler-obs
for ((i=1;i&lt;=5;i++));
do
    if [ ! -d openeuler-obs ];then
    	set +e
        #git clone --depth 1 https://gitee.com/wangchong1995924/openeuler-obs
        git clone --depth 1 https://gitee.com/openeuler/openeuler-obs
        set -e
    else
        break
    fi
done
cd openeuler-obs/tools/

python3 check_package_binary_archive.py -p ${obs_project_name} -dp ${dep_project} -dr ${repo_name} -a ${arch} -ip ${OBS_BACKEND_SERVER_IP} -ou ${OBS_BACKEND_USR} -opwd ${OBS_BACKEND_PWD} -eu ${email_user} -epwd ${email_pwd} -tu ${to_addr}


</command>
