<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>验证、确保obs_meta中的包都存在于obs工程中</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <com.sonyericsson.jenkins.plugins.bfa.model.ScannerJobProperty plugin="build-failure-analyzer@1.27.1">
      <doNotScan>false</doNotScan>
    </com.sonyericsson.jenkins.plugins.bfa.model.ScannerJobProperty>
    <com.gitee.jenkins.connection.GiteeConnectionProperty plugin="gitee@1.2.7">
      <giteeConnection>Gitee</giteeConnection>
    </com.gitee.jenkins.connection.GiteeConnectionProperty>
    <com.chikli.hudson.plugin.naginator.NaginatorOptOutProperty plugin="naginator@1.18.1">
      <optOut>false</optOut>
    </com.chikli.hudson.plugin.naginator.NaginatorOptOutProperty>
    <com.sonyericsson.rebuild.RebuildSettings plugin="rebuild@1.31">
      <autoRebuild>false</autoRebuild>
      <rebuildDisabled>false</rebuildDisabled>
    </com.sonyericsson.rebuild.RebuildSettings>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>500</daysToKeep>
        <numToKeep>1500</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.StringParameterDefinition>
          <name>OBS_SOURCE_HOST</name>
          <defaultValue>172.16.1.89</defaultValue>
          <trim>false</trim>
        </hudson.model.StringParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <assignedNode>k8s-aarch64-openeuler</assignedNode>
  <canRoam>false</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.TimerTrigger>
      <spec>H 14 * * *</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>git config --global http.sslVerify false
git config --global http.postBuffer 52428800

if [ ! -d $WORKSPACE/obs_meta ];then
    cd $WORKSPACE
    for((i=1;i&lt;=5;i++));
    do
    	set +e
        git clone -b master https://$GiteeCloneUserName:$GiteeClonePassword@gitee.com/src-openeuler/obs_meta
        set -e
        if [ -d obs_meta ];then
            if [ &quot;`ls -A obs_meta`&quot; != &quot;&quot; ];then 
        		break
			fi
            rm -rf obs_meta
        fi
    done
else
    rm -rf $WORKSPACE/obs_meta
    cd $WORKSPACE
    for((i=1;i&lt;=5;i++));
    do
    	set +e
        git clone -b master https://$GiteeCloneUserName:$GiteeClonePassword@gitee.com/src-openeuler/obs_meta
        set -e
        if [ -d obs_meta ];then
            if [ &quot;`ls -A obs_meta`&quot; != &quot;&quot; ];then 
        		break
			fi
            rm -rf obs_meta
        fi
    done
fi

sudo su - root &lt;&lt;EOF

git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;

touch ~/.oscrc
cat &gt; ~/.oscrc &lt;&lt; FEOF
[general]
apiurl = http://117.78.1.88
[http://117.78.1.88]
user = $OBS_USER
pass = $OBS_PWD
FEOF


cd $WORKSPACE &amp;&amp; rm -rf openeuler_obs-*


wget http://172.16.1.95:88/mkb/openeuler-obs/openeuler_obs-v1.4.4-aarch64.tar.gz
tar -xvf openeuler_obs-v1.4.4-aarch64.tar.gz &amp;&amp; cd openeuler_obs-v1.4.4-aarch64

#./openeuler_obs -r obs_meta -o $WORKSPACE/obs_meta -ip ${OBS_SOURCE_HOST} -suser root -spwd ${OBS_SOURCE_PWD} -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword} --check_meta Ture
./openeuler_obs -r obs_meta -o $WORKSPACE/obs_meta -ip ${OBS_SOURCE_HOST} -suser root -spwd ${OBS_SOURCE_PWD} -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword} --check_meta Ture --sync_code &quot;&quot;
EOF</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers>
    <org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper plugin="credentials-binding@1.27">
      <bindings>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>openeuler_ci_bot</credentialsId>
          <usernameVariable>GiteeCloneUserName</usernameVariable>
          <passwordVariable>GiteeClonePassword</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>obs_ci_bot</credentialsId>
          <usernameVariable>OBS_USER</usernameVariable>
          <passwordVariable>OBS_PWD</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>d06a4ace-be97-4230-9acd-95c48d6971ad</credentialsId>
          <usernameVariable>OBS_SOURCE_IP</usernameVariable>
          <passwordVariable>OBS_SOURCE_PWD</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
      </bindings>
    </org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper>
  </buildWrappers>
</project>