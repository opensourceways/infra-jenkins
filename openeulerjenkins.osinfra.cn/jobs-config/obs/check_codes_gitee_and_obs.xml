    <hudson.tasks.Shell>
      <command>
sudo su - root &lt;&lt;FEOF

echo &quot;nameserver 114.114.114.114&quot; &gt;&gt; /etc/resolv.conf

if [[ ${branch} == &quot;openEuler-20.03-LTS-Next&quot; ]];then
    exit 0
fi


if [ &quot;x$isbuild&quot; == &quot;x0&quot; ]; then
    echo &quot;skip by all&quot;
    exit 0
fi
(
cat &lt;&lt;EOF
[general]
apiurl=http://117.78.1.88
no_verify=yes
[http://117.78.1.88]
user=${OBS_USER}
pass=${OBS_PWD}
EOF
) &gt; ~/.oscrc

yum install diffutils -y
set +e
git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;
git config --global pull.ff only

#osc list ${obs_project_name} &gt; pkgslist

rm -rf openeuler_obs-*
set -e
#
wget http://172.16.1.95:88/mkb/openeuler-obs/openeuler_obs-v1.4.4-aarch64.tar.gz
tar -xvf openeuler_obs-v1.4.4-aarch64.tar.gz &amp;&amp; cd openeuler_obs-v1.4.4-aarch64/
#./trigger_services ${obs_project_name} ../../pkgslist

./openeuler_obs -cc True -p ${obs_project_name} -b $branch -ip ${OBS_SOURCE_IP} -suser ${SRC_SERVER} -spwd ${SRC_SERVER_PWD} -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword}


FEOF</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
