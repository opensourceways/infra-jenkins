  <builders>
    <hudson.tasks.Shell>
      <command>git config --global http.sslVerify false
git config --global http.postBuffer 52428800
git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;
git config --global pull.ff only

if [ ! -d $WORKSPACE/obs_meta ];then
    cd $WORKSPACE
    for((i=1;i&lt;=5;i++));
    do
    	set +e
        git clone -b master https://$GiteeCloneUserName:$GiteeClonePassword@gitee.com/src-openeuler/obs_meta
        set -e
        if [ -d obs_meta ];then
            if [ &quot;`ls -A obs_meta`&quot; != &quot;&quot; ];then 
        		cd obs_meta &amp;&amp; git pull &amp;&amp; cd -
                break
			fi
            rm -rf obs_meta
        fi
    done
else
    rm -rf $WORKSPACE/obs_meta
    cd $WORKSPACE
    for((i=1;i&lt;=5;i++));
    do
    	set +e
        git clone -b master https://$GiteeCloneUserName:$GiteeClonePassword@gitee.com/src-openeuler/obs_meta
        set -e
        if [ -d obs_meta ];then
            if [ &quot;`ls -A obs_meta`&quot; != &quot;&quot; ];then 
        		cd obs_meta &amp;&amp; git pull &amp;&amp; cd -
                break
			fi
            rm -rf obs_meta
        fi
    done
fi

if [ ! -d $WORKSPACE/obs_meta ];then
	echo &quot;ERROR: git clone obs_meta failed.&quot;
    exit 1
fi

sudo su - root &lt;&lt;EOF

git config --global http.sslVerify false
git config --global http.postBuffer 52428800
git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;
git config --global pull.ff only



touch ~/.oscrc
cat &gt; ~/.oscrc &lt;&lt; FEOF
[general]
apiurl = http://117.78.1.88
[http://117.78.1.88]
user = $OBS_USER
pass = $OBS_PWD
FEOF

cat ~/.oscrc


cd $WORKSPACE/obs_meta &amp;&amp; git reset --hard ${giteePullRequestLastCommit} &amp;&amp; cd -

cd $WORKSPACE &amp;&amp; rm -rf openeuler_obs-*
#wget http://172.16.1.95:88/mkb/openeuler-obs/openeuler_obs-v1.3.4-aarch64.tar.gz
#tar -xvf openeuler_obs-v1.3.4-aarch64.tar.gz &amp;&amp; cd openeuler_obs-v1.3.4-aarch64

wget http://172.16.1.95:88/mkb/openeuler-obs/openeuler_obs-v1.4.6-aarch64.tar.gz
tar -xvf openeuler_obs-v1.4.6-aarch64.tar.gz &amp;&amp; cd openeuler_obs-v1.4.6-aarch64

./openeuler_obs -r obs_meta -o $WORKSPACE/obs_meta -ip ${OBS_SOURCE_HOST} -suser ${SOURCE_USER} -spwd ${SOURCE_PWD} -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword} -cmid ${giteePullRequestLastCommit}
#./openeuler_obs -r obs_meta -o $WORKSPACE/obs_meta -ip ${OBS_SOURCE_HOST} -suser ${SOURCE_USER} -spwd ${SOURCE_PWD} -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword} -cmid ${giteePullRequestLastCommit} --sync_code &quot;&quot;
EOF</command>
