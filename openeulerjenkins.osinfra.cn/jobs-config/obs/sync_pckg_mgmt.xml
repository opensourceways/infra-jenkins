    <hudson.tasks.Shell>
      <command>if [[ &quot;${isbuild}&quot; != &quot;1&quot; ]];then
	exit 0
fi
git config --global http.sslVerify false
git config --global http.postBuffer 52428800
git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;
git config --global pull.ff only

sudo su - root &lt;&lt;EOF
if [ -d $WORKSPACE/obs_meta ];then
	rm -rf $WORKSPACE/obs_meta
fi

if [ -d $WORKSPACE/release-management ];then
	rm -rf $WORKSPACE/release-management
fi
EOF

if [ ! -d $WORKSPACE/obs_meta ];then
    cd $WORKSPACE
    for((i=1;i&lt;=5;i++));
    do
    	set +e
        git clone -b master https://$GiteeCloneUserName:$GiteeClonePassword@gitee.com/src-openeuler/obs_meta
        set -e
        if [ -d obs_meta ];then
            if [ &quot;`ls -A obs_meta`&quot; != &quot;&quot; ];then 
        		break
			else
            	rm -rf obs_meta
            fi
        fi
    done
    cd -
fi


if [ ! -d $WORKSPACE/release-management ];then
    cd $WORKSPACE
    for((i=1;i&lt;=5;i++));
    do
    	set +e
        git clone -b master https://$GiteeCloneUserName:$GiteeClonePassword@gitee.com/openeuler/release-management
        set -e
        if [ -d release-management ];then
            if [ &quot;`ls -A release-management`&quot; != &quot;&quot; ];then 
        		cd release-management &amp;&amp; git reset --hard ${giteePullRequestLastCommit} &amp;&amp; cd -
                #cd release-management &amp;&amp; git reset --hard 7a1e553d75ead96baeb1b618ca7041e2ab462142 &amp;&amp; cd -
                break
			else
            	rm -rf release-management
            fi
        fi
    done
    cd -
fi


sudo su - root &lt;&lt;EOF

git config --global http.sslVerify false
git config --global http.postBuffer 52428800
git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;
git config --global pull.ff only


cd $WORKSPACE &amp;&amp; rm -rf openeuler_obs-*
wget http://172.16.1.95:88/mkb/openeuler-obs/openeuler_obs-v1.4.4-aarch64.tar.gz
tar -xvf openeuler_obs-v1.4.4-aarch64.tar.gz &amp;&amp; cd openeuler_obs-v1.4.4-aarch64

./openeuler_obs -o $WORKSPACE/obs_meta -rm $WORKSPACE/release-management -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword} --pckg_mgmt true
#./openeuler_obs -o $WORKSPACE/obs_meta -rm $WORKSPACE/release-management -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword} --pckg_mgmt true --sync_code &quot;&quot;
