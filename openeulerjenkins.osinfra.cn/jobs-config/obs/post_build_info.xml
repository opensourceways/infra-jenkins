        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>obs_project_name</name>
          <description>obs 项目名称</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>openEuler-Mainline-copy</string>
              <string>openEuler:Factory</string>
              <string>openEuler:Mainline</string>
              <string>openEuler:Epol</string>
              <string>openEuler:Extras</string>
              <string>openEuler:20.03:LTS</string>
              <string>openEuler:20.03:LTS:Next</string>
              <string>openEuler:20.03:LTS:Next:Epol</string>
              <string>openEuler:20.03:LTS:SP1</string>
              <string>openEuler:20.09</string>
              <string>openEuler:20.09:Epol</string>
              <string>openEuler:20.03:LTS:SP1:Epol</string>
              <string>openEuler:21.03</string>
              <string>openEuler:21.03:Epol</string>
              <string>openEuler:20.03:LTS:SP2</string>
              <string>openEuler:20.03:LTS:SP2:Epol</string>
              <string>openEuler:21.09</string>
              <string>openEuler:21.09:Epol</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <assignedNode>k8s-aarch64-openeuler</assignedNode>
  <canRoam>false</canRoam>
  <disabled>true</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.TimerTrigger>
      <spec>H H/12 * * *
</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>true</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>
(
cat &lt;&lt;EOF
[general]
apiurl=http://117.78.1.88
no_verify=yes
[http://117.78.1.88]
user=${OBS_USER}
pass=${OBS_PWD}
EOF
) &gt; ~/.oscrc

set +e
git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;
git config --global pull.ff only

osc rebuild --all ${obs_project_name} 
obs_build_start=`date &quot;+%Y-%m-%d %H:%M:%S&quot;`

#rm -rf next-test
#git clone --depth 1 https://gitee.com/miao_kaibo/next-test
flag=0
while [ $flag -eq 0 ]
do
    set +e
    osc prjresults ${obs_project_name} --csv | grep &quot;aarch64/published&quot; | grep &quot;x86_64/published&quot;
    if [ $? -eq 0 ];then
        flag=1
        break
    fi
    sleep 5
    set -e
done

obs_build_end=`date &quot;+%Y-%m-%d %H:%M:%S&quot;`

iso_start=`date &quot;+%Y-%m-%d %H:%M:%S&quot;`
iso_end=${iso_start}
rm -rf openeuler-obs
wget http://172.16.1.95:88/mkb/openeuler-obs/openeuler_obs-v1.3.9-aarch64.tar.gz
tar -zxvf openeuler_obs-v1.3.9-aarch64.tar.gz
mv openeuler_obs-v1.3.9-aarch64/tools/build_obs_iso ./
chmod 777 build_obs_iso &amp;&amp; ./build_obs_iso -f kafka -b 159.138.58.212:9092 -t &quot;openeuler_statewall_dist_build_time&quot; -p ${obs_project_name} -pst &quot;${obs_build_start}&quot; -pet &quot;${obs_build_end}&quot; -ist &quot;${iso_start}&quot; -iet &quot;${iso_end}&quot;
</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers>
    <org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper plugin="credentials-binding@1.27">
