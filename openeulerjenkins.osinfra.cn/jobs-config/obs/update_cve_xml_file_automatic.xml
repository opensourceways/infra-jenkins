<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>每隔6小时自动更新https://repo.openeuler.org/security/data/cvrf目录下CVE的xml文件和index.txt文件</description>
  <keepDependencies>false</keepDependencies>
  <properties>
