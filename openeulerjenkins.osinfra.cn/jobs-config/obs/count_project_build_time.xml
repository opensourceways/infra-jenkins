<?xml version='1.1' encoding='UTF-8'?>
<project>
  <actions/>
  <description>归档OBS工程二进制到依赖仓库, 统计工程构建时间</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.model.BuildDiscarderProperty>
      <strategy class="hudson.tasks.LogRotator">
        <daysToKeep>100</daysToKeep>
        <numToKeep>1500</numToKeep>
        <artifactDaysToKeep>-1</artifactDaysToKeep>
        <artifactNumToKeep>-1</artifactNumToKeep>
      </strategy>
    </jenkins.model.BuildDiscarderProperty>
    <com.sonyericsson.jenkins.plugins.bfa.model.ScannerJobProperty plugin="build-failure-analyzer@1.27.1">
      <doNotScan>false</doNotScan>
    </com.sonyericsson.jenkins.plugins.bfa.model.ScannerJobProperty>
    <com.gitee.jenkins.connection.GiteeConnectionProperty plugin="gitee@1.2.7">
      <giteeConnection>Gitee</giteeConnection>
    </com.gitee.jenkins.connection.GiteeConnectionProperty>
    <com.chikli.hudson.plugin.naginator.NaginatorOptOutProperty plugin="naginator@1.18.1">
      <optOut>false</optOut>
    </com.chikli.hudson.plugin.naginator.NaginatorOptOutProperty>
    <com.sonyericsson.rebuild.RebuildSettings plugin="rebuild@1.31">
      <autoRebuild>false</autoRebuild>
      <rebuildDisabled>false</rebuildDisabled>
    </com.sonyericsson.rebuild.RebuildSettings>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.ChoiceParameterDefinition>
          <name>obs_project_name</name>
          <description>obs 项目名称</description>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>openEuler:Mainline</string>
              <string>openEuler:20.03:LTS</string>
              <string>openEuler:20.03:LTS:Next</string>
              <string>openEuler:20.03:LTS:SP1</string>
              <string>openEuler:20.09</string>
              <string>openEuler:21.03</string>
              <string>openEuler:21.09</string>
              <string>openEuler:20.03:LTS:SP2</string>
              <string>openEuler:20.03:LTS:SP3</string>
              <string>openEuler:22.03:LTS:Next</string>
              <string>openEuler-Mainline-copy</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
        <hudson.model.ChoiceParameterDefinition>
          <name>repo</name>
          <choices class="java.util.Arrays$ArrayList">
            <a class="string-array">
              <string>standard_aarch64</string>
              <string>standard_x86_64</string>
            </a>
          </choices>
        </hudson.model.ChoiceParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <assignedNode>k8s-aarch64-openeuler</assignedNode>
  <canRoam>false</canRoam>
  <disabled>true</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>true</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>sudo su - root &lt;&lt;FEOF
echo &quot;nameserver 114.114.114.114&quot; &gt;&gt; /etc/resolv.conf
FEOF

(
cat &lt;&lt;EOF
[general]
apiurl=http://117.78.1.88
no_verify=yes
[http://117.78.1.88]
user=${OBS_USER}
pass=${OBS_PWD}
EOF
) &gt; ~/.oscrc

set +e
git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;
git config --global pull.ff only

if [[ ${repo} =~ &quot;x86_64&quot; ]];then
	exit 0
fi

flag=0
while [ $flag -eq 0 ]
do
    set +e
    osc prjresults ${obs_project_name} --csv | grep &quot;aarch64/published&quot; | grep &quot;x86_64/published&quot;
    if [ $? -eq 0 ];then
        flag=1
        break
    fi
    sleep 5
    set -e
done

obs_build_end=`date &quot;+%Y-%m-%d %H:%M:%S&quot;`

echo &quot;build_end:${obs_build_end}&quot;
set -e


</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers>
    <org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper plugin="credentials-binding@1.27">
      <bindings>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>openeuler_ci_bot</credentialsId>
          <usernameVariable>GiteeCloneUserName</usernameVariable>
          <passwordVariable>GiteeClonePassword</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>5a700b90-012d-48a8-b6c6-64a5ebaf82e9</credentialsId>
          <usernameVariable>OBS_BACKEND_USR_95</usernameVariable>
          <passwordVariable>OBS_BACKEND_PWD_95</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>obs_ci_bot</credentialsId>
          <usernameVariable>OBS_USER</usernameVariable>
          <passwordVariable>OBS_PWD</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>d4071859-e366-40c4-995e-35881b9f2645</credentialsId>
          <usernameVariable>OBS_BACKEND_USR_124</usernameVariable>
          <passwordVariable>OBS_BACKEND_PWD_124</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>172.16.1.43</credentialsId>
          <usernameVariable>OBS_BACKEND_USR_43</usernameVariable>
          <passwordVariable>OBS_BACKEND_PWD_43</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>172.16.1.110</credentialsId>
          <usernameVariable>OBS_BACKEND_USR_110</usernameVariable>
          <passwordVariable>OBS_BACKEND_PWD_110</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
      </bindings>
    </org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper>
  </buildWrappers>
</project>