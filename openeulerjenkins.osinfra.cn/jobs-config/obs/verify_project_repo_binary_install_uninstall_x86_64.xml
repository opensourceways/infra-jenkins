    <hudson.tasks.Shell>
      <command>sudo su - root &lt;&lt;FEOF
echo &quot;nameserver 114.114.114.114&quot; &gt;&gt; /etc/resolv.conf

touch ~/.oscrc
cat &gt; ~/.oscrc &lt;&lt; EOF
[general]
apiurl = http://117.78.1.88
[http://117.78.1.88]
user = $OBS_USER
pass = $OBS_PWD
EOF

set +e
git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;
git config --global pull.ff only

if [[ ${arch} != &quot;x86_64&quot; ]];then
	echo &quot;arch must be x86_64&quot;
    exit 1
fi

if [[ &quot;${email_addr}&quot; == &quot;&quot; ]];then
	echo &quot;email_addr is empty!&quot;
    exit 1
fi

if [[ &quot;${branch}&quot; == &quot;&quot; ]];then
	echo &quot;branch is empty!&quot;
    exit 1
fi

if [[ &quot;${repo_project}&quot; == &quot;&quot; ]];then
	echo &quot;repo_project is empty!&quot;
    exit 1
fi

if [ &quot;${OBS_BACKEND_SERVER_IP}&quot; == &quot;&quot; ];then
	echo &quot;OBS_BACKEND_SERVER_IP is empty!&quot;
    exit 1
fi

rm -rf openeuler-obs
for ((i=1;i&lt;=5;i++));
do
    if [ ! -d openeuler-obs ];then
    	set +e
        #git clone --depth 1 https://gitee.com/wangchong1995924/openeuler-obs
        git clone --depth 1 https://gitee.com/openeuler/openeuler-obs
        set -e
    else
        break
    fi
done

cd openeuler-obs/tools/

if [ -z ${check_project_name} ];then
	python3 verify_repo_binary_install_uninstall.py  -br ${branch} -a ${arch} -ip ${OBS_BACKEND_SERVER_IP} -eu ${email_user} -epwd ${email_pwd} -tu ${email_addr} -rp &quot;${repo_project}&quot;
else
	python3 verify_repo_binary_install_uninstall.py  -br ${branch} -a ${arch} -ip ${OBS_BACKEND_SERVER_IP} -eu ${email_user} -epwd ${email_pwd} -tu ${email_addr} -rp &quot;${repo_project}&quot; -cp ${check_project_name}
fi
FEOF</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
