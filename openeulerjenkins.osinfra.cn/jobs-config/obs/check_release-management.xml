  <builders>
    <hudson.tasks.Shell>
      <command>

git config --global http.postBuffer 52428800


touch ~/.oscrc
cat &gt; ~/.oscrc &lt;&lt; FEOF
[general]
apiurl = http://117.78.1.88
[http://117.78.1.88]
user = $OBS_USER
pass = $OBS_PWD
FEOF


cat ~/.oscrc

if [ -d $WORKSPACE/obs_meta ];then
	sudo rm -rf $WORKSPACE/obs_meta
fi

if [ -d $WORKSPACE/release-management ];then
	sudo rm -rf $WORKSPACE/release-management
fi

if [ ! -d $WORKSPACE/obs_meta ];then
    cd $WORKSPACE
    for((i=1;i&lt;=5;i++));
    do
    	set +e
        git clone -b master https://$GiteeCloneUserName:$GiteeClonePassword@gitee.com/src-openeuler/obs_meta
        set -e
        if [ -d obs_meta ];then
            if [ &quot;`ls -A obs_meta`&quot; != &quot;&quot; ];then 
        		break
			else
            	sudo rm -rf obs_meta
            fi
        fi
    done
    cd -
fi


if [ ! -d $WORKSPACE/release-management ];then
    cd $WORKSPACE
    for((i=1;i&lt;=5;i++));
    do
    	set +e
        git clone -b master https://$GiteeCloneUserName:$GiteeClonePassword@gitee.com/openeuler/release-management

        set -e
        if [ -d release-management ];then
            if [ &quot;`ls -A release-management`&quot; != &quot;&quot; ];then 
        		break
			else
            	sudo rm -rf release-management
            fi
        fi
    done
    cd -
fi

sudo su - root &lt;&lt;EOF

git config --global user.email &quot;80474298@qq.com&quot;
git config --global user.name &quot;openeuler-ci-bot&quot;
git config --global pull.ff only
set +e
rm -rf openeuler_obs-*
set -
wget http://172.16.1.95:88/mkb/openeuler-obs/openeuler_obs-v1.4.4-aarch64.tar.gz
tar -xvf openeuler_obs-v1.4.4-aarch64.tar.gz &amp;&amp; cd openeuler_obs-v1.4.4-aarch64
./openeuler_obs  -cpm true -o $WORKSPACE/obs_meta -rm $WORKSPACE/release-management -prid $giteePullRequestIid  -guser ${GiteeCloneUserName} -gpwd ${GiteeClonePassword}
EOF

</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
