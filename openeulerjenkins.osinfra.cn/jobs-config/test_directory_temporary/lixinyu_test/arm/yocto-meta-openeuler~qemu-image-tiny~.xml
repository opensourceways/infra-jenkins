  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command># 将结果写入远程文件
arch=&quot;arm_~qemu-image-tiny~&quot;
fileserver_tmpfile_path=&quot;/repo/soe/check_item&quot;
comment_file=${repo}_${prid}_${arch}_test_comment
if [ $exitCode = 0 ]; then
	echo &quot;[{&apos;name&apos;: &apos;check_test&apos;, &apos;result&apos;: 0}]&quot; &gt; ${comment_file}
else
	echo &quot;[{&apos;name&apos;: &apos;check_test&apos;, &apos;result&apos;: 2}]&quot; &gt; ${comment_file}
fi
if [[ -f ${comment_file} ]]; then
	scp -r -i ${SaveBuildRPM2Repo} -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null -o LogLevel=ERROR ${comment_file} root@${repo_server}:$fileserver_tmpfile_path
    echo &quot;upload success&quot;
fi

exit $exitCode
</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers>
    <org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper plugin="credentials-binding@1.27">
