cat &gt; hotfix.py &lt;&lt; EOF
#!/usr/bin/python3
from src.proxy.jenkins_proxy import JenkinsProxy
import xml.etree.ElementTree as ET
jp = JenkinsProxy(&quot;http://jenkins.jenkins&quot;, &quot;$user&quot;, &quot;$token&quot;)
config = jp.get_config(&quot;$job&quot;)
new_command = open(&apos;shell_command.sh&apos;).read( )
root = ET.fromstring(config)
ele = root.findall(&quot;builders/hudson.tasks.Shell/command&quot;)
if ele:
    print(ele[0].text)
    ele[0].text = new_command
config = ET.tostring(root).decode(&quot;utf-8&quot;)

print(new_command)
print(config)
jp.update_job(&quot;$job&quot;, config)
EOF


rm -rf /home/jenkins/ci_check2
git clone -b master https://gitee.com/openeuler/openeuler-jenkins /home/jenkins/ci_check2

export PYTHONPATH=/home/jenkins/ci_check2
python3 hotfix.py</command>
