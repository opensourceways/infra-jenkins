  <builders>
    <hudson.tasks.Shell>
      <command>sudo su - root &lt;&lt;EOF

touch ~/.oscrc
cat &gt; ~/.oscrc &lt;&lt; FEOF
[general]
apiurl = http://117.78.1.88
[http://117.78.1.88]
user = ${OBS_USER}
pass = ${OBS_PWD}
FEOF
cat ~/.oscrc

#osc ls
#osc rebuild ${project} --all

#begintime=`date`
begintime=&quot;09:55&quot;
while true
do
	res=$(osc prjresults {project} --csv | grep &quot;aarch64/published&quot; | grep &quot;x86_64/published&quot;)
	if [ -z &quot;${res}&quot; ];then
    	continue
    else
    	endtime=`date`
        break
	fi
done

echo &quot;${project}工程全量构建时间记录如下：&quot;
echo &quot;开始时间：${begintime}&quot;
echo &quot;结束时间：${endtime}&quot;

EOF</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
