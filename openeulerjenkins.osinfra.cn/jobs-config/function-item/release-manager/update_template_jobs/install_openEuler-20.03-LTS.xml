    <hudson.tasks.Shell>
      <command>sudo su - root &lt;&lt; EOF
cd /root
if [ -d /root/check_pkg_install_uninstall_function_service ]; then
	rm -rf /root/check_pkg_install_uninstall_function_service
fi
#git clone https://gitee.com/angela7/check_pkg_install_uninstall_function_service.git

git clone https://gitee.com/tushenmei/check_pkg_install_uninstall_function_service.git

cd check_pkg_install_uninstall_function_service

rm -rf ./pkglist.txt
echo ${PKGLIST} &gt; ./pkglist.txt
sed -i &quot;s/,/\\n/g&quot; ./pkglist.txt


# oscrc
mkdir -p /root/.config/osc
&gt; /root/.config/osc/oscrc
cat &gt; /root/.config/osc/oscrc &lt;&lt; FEOF3
[general]
apiurl=http://117.78.1.88
no_verify=1

[http://117.78.1.88]
user=$OBS_USER
pass=$OBS_PASSWD
FEOF3

# check.config
sed -i &quot;s#yum_repo=.*#yum_repo=/root/update-release.repo#g&quot; /root/check_pkg_install_uninstall_function_service/check.config
sed -i &quot;s#obs_repo=.*#obs_repo=standard_${ARCH}#g&quot; /root/check_pkg_install_uninstall_function_service/check.config
sed -i &quot;s#obs_arch=.*#obs_arch=${ARCH}#g&quot; /root/check_pkg_install_uninstall_function_service/check.config

# yum repo
&gt; /root/update-release.repo
cat &gt; /root/update-release.repo &lt;&lt; FEOF4
[OS]
name=OS
baseurl=https://repo.openeuler.org/${BRANCH}/OS/${ARCH}/
enabled=1
gpgcheck=0

[everything]
name=everything
baseurl=https://repo.openeuler.org/${BRANCH}/everything/${ARCH}/
enabled=1
gpgcheck=0

[epol]
name=epol
baseurl=https://repo.openeuler.org/${BRANCH}/EPOL/${ARCH}/
enabled=1
gpgcheck=0

[update]
name=update
baseurl=https://repo.openeuler.org/${BRANCH}/update/${ARCH}/
enabled=1
gpgcheck=0

[update_${UPDATE_TIME}]
name=update_${UPDATE_TIME}
baseurl=http://121.36.84.172/repo.openeuler.org/${BRANCH}/update_${UPDATE_TIME}/${ARCH}/
enabled=1
gpgcheck=0
priority=3
FEOF4


sed -i &apos;s/\/etc\/pkgship-tools\/check.config/.\/check.config/g&apos; check_pkg_basic_usage.sh
sed -i &apos;s/\/etc\/pkgship-tools\/check.config/.\/check.config/g&apos; check_pkg_basic_usage_indocker.sh
#sed -i &apos;/rpm\=\${rpm\%\\\-\*}/d&apos; check_pkg_basic_usage.sh

dnf install osc -y

# obs project
if [ $EPOL = &quot;true&quot; ]; then
	bash check_pkg_basic_usage.sh -i --without-docker -l pkglist.txt -p ${BRANCH//-/:}:Epol
else
    bash -x check_pkg_basic_usage.sh -i --without-docker -l pkglist.txt -p ${BRANCH//-/:}
fi

# install esdk-obs-python
cd
yum install wget -y
yum install tar -y
wget https://repo.huaweicloud.com/repository/pypi/packages/89/b1/a1361a2a1a115fbe33a4d423d48637aecf05c48f533c28b9adb3b4953639/esdk-obs-python-3.20.11.tar.gz#sha256=1446d25aca18fec95823b3e9e5eaff4426f227b23a6fd2244b5ef582a9698f4c
tar -zxvf esdk-obs-python-3.20.11.tar.gz
cd esdk-obs-python-3.20.11/
python3 setup.py install

cd
if [ -d /root/release-tools-test ]; then
	rm -rf /root/release-tools-test
fi
git clone https://gitee.com/angela7/release-tools-test.git
export PYTHONPATH=/root/release-tools-test/release-assistant/
python3 release-tools-test/release-assistant/javcra/api/obscloud.py --choice check_result --ak ${AK} --sk ${SK} --branch ${BRANCH} --path /root

EOF</command>
