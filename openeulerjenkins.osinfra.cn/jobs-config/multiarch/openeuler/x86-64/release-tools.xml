  <builders>
    <hudson.tasks.Shell>
      <command>echo &quot;start release-tools ci&quot;

REPO_CONFIG_FILE=&quot;/etc/yum.repos.d/openEuler_release_tools.repo&quot;
if [ ! -f ${REPO_CONFIG_FILE} ]; then
     sudo touch ${REPO_CONFIG_FILE}
  fi
sudo bash -c &quot;cat&gt;${REPO_CONFIG_FILE}&quot;&lt;&lt;EOF
[sp2]
name=sp2
baseurl=http://124.70.10.167:82/openEuler:/20.03:/LTS:/SP2/standard_aarch64/
enabled=1
gpgcheck=0
EOF

sudo yum install python-jenkins -y 
sudo yum install python3-requests -y
sudo yum install python-concurrent-log-handler -y
sudo yum install python3-gevent -y
sudo yum install python3-marshmallow -y 
sudo yum install python3-pyyaml -y
sudo yum install python-pandas -y
sudo yum install python-xlrd -y
sudo yum install python-retrying -y
sudo yum install python-esdk-obs-python -y
sudo yum install python-coverage -y


init_path=`pwd`
export PYTHONPATH=$init_path/release-tools/release-assistant/

python3 release-tools/release-assistant/test/coverage_count.py
</command>
      <configuredLocalRules/>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers>
    <org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper plugin="credentials-binding@1.27">
      <bindings>
        <org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
          <credentialsId>openeuler_ci_bot</credentialsId>
          <usernameVariable>GiteeUserName</usernameVariable>
          <passwordVariable>GiteePassword</passwordVariable>
        </org.jenkinsci.plugins.credentialsbinding.impl.UsernamePasswordMultiBinding>
      </bindings>
    </org.jenkinsci.plugins.credentialsbinding.impl.SecretBuildWrapper>
    <hudson.plugins.ansicolor.AnsiColorBuildWrapper plugin="ansicolor@0.7.1">
      <colorMapName>vga</colorMapName>
    </hudson.plugins.ansicolor.AnsiColorBuildWrapper>
  </buildWrappers>
</project>