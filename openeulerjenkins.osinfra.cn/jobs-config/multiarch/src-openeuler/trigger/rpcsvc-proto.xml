              <configs>
                <hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
                  <properties>repo=${giteeRepoName}
branch=${giteeSourceBranch}
tbranch=${giteeTargetBranch}
prid=${giteePullRequestIid}
committer=${giteeCommitter}
commentid=${commentID}
prtitle=${giteePullRequestTitle}
jobtriggertime=${jobTriggerTime}
prcreatetime=${prCreateTime}
prurl=${prUrl}
triggerbuildid=${BUILD_ID}
ACL=${ACL}</properties>
                  <textParamValueOnNewLine>false</textParamValueOnNewLine>
                </hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
              </configs>
              <projects>multiarch/src-openeuler/comment/rpcsvc-proto</projects>
              <condition>ALWAYS</condition>
              <triggerWithNoParameters>false</triggerWithNoParameters>
              <triggerFromChildProjects>false</triggerFromChildProjects>
            </hudson.plugins.parameterizedtrigger.BuildTriggerConfig>
          </configs>
        </hudson.plugins.parameterizedtrigger.BuildTrigger>
