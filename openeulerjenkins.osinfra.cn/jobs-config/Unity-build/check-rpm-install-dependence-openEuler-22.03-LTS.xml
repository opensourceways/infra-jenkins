  <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@2.94">
    <script>pipeline {
    agent none
    stages(&quot;begin&quot;) {
		stage(&quot;check install&quot;) {
                parallel{
                    stage(&quot;2203_lts_aarch64&quot;){
                        steps{
                            build job: &apos;check_rpm_install_dependence_standard_aarch64&apos;, parameters: [string(name: &apos;OBS_STANDARD_PROJECT&apos;,value: params.obs_standard_prj), string(name: &apos;OBS_SERVER_IP&apos;,value: params.obs_server_ip)], propagate: false
                        }
                    }
                    stage(&quot;2203_lts_epol_aarch64&quot;){
                        steps{
                            build job: &apos;check_rpm_install_dependence_epol_aarch64&apos;, parameters: [string(name: &apos;OBS_STANDARD_PROJECT&apos;,value: params.obs_epol_prj), string(name: &apos;OBS_SERVER_IP&apos;,value: params.obs_server_ip)], propagate: false
                        }
                    }
                    stage(&quot;2203_lts_x86_64&quot;){
                        steps{
                            build job: &apos;check_rpm_install_dependence_standard_x86_64&apos;, parameters: [string(name: &apos;OBS_STANDARD_PROJECT&apos;,value: params.obs_standard_prj), string(name: &apos;OBS_SERVER_IP&apos;,value: params.obs_server_ip)], propagate: false
                        }
                    }   
                    stage(&quot;2203_lts_epol_x86_64&quot;){
                        steps{
                            build job: &apos;check_rpm_install_dependence_epol_x86_64&apos;, parameters: [string(name: &apos;OBS_STANDARD_PROJECT&apos;,value: params.obs_epol_prj), string(name: &apos;OBS_SERVER_IP&apos;,value: params.obs_server_ip)], propagate: false
                        }
                    }
                }
		}
    }
}</script>
    <sandbox>true</sandbox>
  </definition>
  <triggers/>
  <disabled>false</disabled>
</flow-definition>